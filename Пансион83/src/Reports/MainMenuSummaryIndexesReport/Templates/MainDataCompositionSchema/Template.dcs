	<calculatedField>
		<dataPath>RoomsAvailable</dataPath>
		<expression>TotalRoomsClosingBalance- RoomsBlockedClosingBalance</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Номеров в продаже</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Rooms available</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>de</v8:lang>
				<v8:content>Zimmer im Verkauf</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:decimal</v8:Type>
			<v8:NumberQualifiers>
				<v8:Digits>10</v8:Digits>
				<v8:FractionDigits>0</v8:FractionDigits>
				<v8:AllowedSign>Any</v8:AllowedSign>
			</v8:NumberQualifiers>
		</valueType>
		<inputParameters>
			<dcscor:item>
				<dcscor:parameter>ФорматРедактирования</dcscor:parameter>
				<dcscor:value xsi:type="xs:string">ND=10; NFD=0; NZ=; NG=</dcscor:value>
			</dcscor:item>
		</inputParameters>
	</calculatedField>
	<calculatedField>
		<dataPath>OccupancyPercent</dataPath>
		<expression>cmRound(ISNULL(100*RoomsRentedTurnover/(TotalRoomsClosingBalance- RoomsBlockedClosingBalance), 0), 2)</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>% Продаж номеров</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Occupancy %</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>de</v8:lang>
				<v8:content>% der Zimmerverkäufe</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:decimal</v8:Type>
			<v8:NumberQualifiers>
				<v8:Digits>6</v8:Digits>
				<v8:FractionDigits>2</v8:FractionDigits>
				<v8:AllowedSign>Any</v8:AllowedSign>
			</v8:NumberQualifiers>
		</valueType>
		<inputParameters>
			<dcscor:item>
				<dcscor:parameter>ФорматРедактирования</dcscor:parameter>
				<dcscor:value xsi:type="xs:string">ND=6; NFD=2; NZ=; NG=</dcscor:value>
			</dcscor:item>
		</inputParameters>
	</calculatedField>
	<calculatedField>
		<dataPath>RevPAR</dataPath>
		<expression>cmRound(ISNULL(RoomRevenueTurnover/(TotalRoomsClosingBalance -RoomsBlockedClosingBalance), 0), 2)</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Сред. доходность номера (RevPAR)</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Rev. per average room (RevPAR)</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>de</v8:lang>
				<v8:content>durchschnittlicher Ertrag des Zimmers (RevRAR)</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:decimal</v8:Type>
			<v8:NumberQualifiers>
				<v8:Digits>17</v8:Digits>
				<v8:FractionDigits>2</v8:FractionDigits>
				<v8:AllowedSign>Any</v8:AllowedSign>
			</v8:NumberQualifiers>
		</valueType>
		<inputParameters>
			<dcscor:item>
				<dcscor:parameter>ФорматРедактирования</dcscor:parameter>
				<dcscor:value xsi:type="xs:string">ND=17; NFD=2; NZ=</dcscor:value>
			</dcscor:item>
		</inputParameters>
	</calculatedField>
	<calculatedField>
		<dataPath>ADR</dataPath>
		<expression>CASE WHEN RoomsRentedTurnover&lt;&gt;0 And RoomRevenueTurnover&lt;&gt;0 Then cmRound(ISNULL(RoomRevenueTurnover/RoomsRentedTurnover, 0), 2) Else 0 End</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Сред. цена за номер (ADR)</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Average Daily Rate (ADR)</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>de</v8:lang>
				<v8:content>Durchschnittspreis pro Zimmer (ADR)</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:decimal</v8:Type>
			<v8:NumberQualifiers>
				<v8:Digits>17</v8:Digits>
				<v8:FractionDigits>2</v8:FractionDigits>
				<v8:AllowedSign>Any</v8:AllowedSign>
			</v8:NumberQualifiers>
		</valueType>
		<inputParameters>
			<dcscor:item>
				<dcscor:parameter>ФорматРедактирования</dcscor:parameter>
				<dcscor:value xsi:type="xs:string">ND=17; NFD=2; NZ=</dcscor:value>
			</dcscor:item>
		</inputParameters>
	</calculatedField>
	<totalField>
		<dataPath>OccupancyPercent</dataPath>
		<expression>cmRound(ISNULL(100*Sum(RoomsRentedTurnover)/Sum(RoomsAvailable), 0), 2)</expression>
	</totalField>
	<totalField>
		<dataPath>RevPAR</dataPath>
		<expression>cmRound(ISNULL(Sum(RoomRevenueTurnover)/Sum(RoomsAvailable), 0), 2)</expression>
	</totalField>
	<totalField>
		<dataPath>RoomsAvailable</dataPath>
		<expression>cmRound(ISNULL(Sum(RoomsAvailable), 0), 0)</expression>
	</totalField>
	<totalField>
		<dataPath>ADR</dataPath>
		<expression>Sum(ADR)</expression>
	</totalField>

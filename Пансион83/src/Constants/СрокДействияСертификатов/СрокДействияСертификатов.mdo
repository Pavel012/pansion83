  <name>СрокДействияСертификатов</name>
  <synonym>
    <key>en</key>
    <value>Number of months when gift certificates are valid</value>
  </synonym>
  <synonym>
    <key>de</key>
    <value>Anzahl von Monaten der Gültigkeit von Geschenkgutscheinen</value>
  </synonym>
